{
  "name": "marine-plastic-explorer",
  "version": "0.0.1",
  "description": "Marine Plastic Explorer",
  "repository": {
    "type": "git",
    "url": "git://github.com/unfolddata/marine-plastic-explorer"
  },
  "engines": {
    "npm": ">=8",
    "node": ">=16.20.2"
  },
  "author": "tmfrnz",
  "license": "MIT",
  "homepage": "/content/",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production APP_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:test": "cross-env NODE_ENV=production APP_ENV=test webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development APP_ENV=test node server",
    "start:pdb": "cross-env NODE_ENV=development APP_ENV=production node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path ./build --ignore-pattern ./build --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path ./build --ignore-pattern ./build --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write",
    "build-jekyll": "jekyll build",
    "build-all": "npm run build && jekyll build",
    "start-jekyll": "jekyll serve",
    "predeploy": "npm run build && npm run predeploy-addjs && npm run predeploy-commitjs",
    "predeploy:test": "npm run build:test && npm run predeploy-addjs && npm run predeploy-commitjs:test",
    "predeploy-addjs": "git add index.html && cd build && git add . && cd ..",
    "predeploy-commitjs": "git commit -m \"update js build\" -n --allow-empty",
    "predeploy-commitjs:test": "git commit -m \"update js build for test repo\" -n --allow-empty",
    "deploy-ghpages": "git push prod gh-pages",
    "deploy-ghpages:test": "git push test HEAD:gh-pages --force",
    "deploy-js": "npm run predeploy && npm run deploy-ghpages",
    "deploy-js:test": "npm run predeploy:test && npm run deploy-ghpages:test"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "@turf/bezier-spline": "^6.0.3",
    "@turf/helpers": "^6.1.4",
    "anchorme": "^2.1.2",
    "chalk": "^2.4.2",
    "compression": "1.7.4",
    "connected-react-router": "6.4.0",
    "cross-env": "5.2.0",
    "csv2geojson": "^5.1.1",
    "d3-scale": "^3.2.1",
    "express": "4.18.2",
    "fontfaceobserver": "2.1.0",
    "grommet": "^2.13.0",
    "history": "4.9.0",
    "hoist-non-react-statics": "3.3.0",
    "immer": "9.0.6",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "js-cookie": "^2.2.1",
    "leaflet": "^1.6.0",
    "leaflet-polylinedecorator": "^1.6.0",
    "leaflet.vectorgrid": "^1.3.0",
    "lodash": "^4.17.21",
    "memoize-one": "^6.0.0",
    "minimist": "^1.2.6",
    "papaparse": "^5.2.0",
    "prop-types": "15.7.2",
    "re-reselect": "^4.0.1",
    "react": "16.8.6",
    "react-csv-downloader": "^2.7.0",
    "react-dom": "16.8.6",
    "react-helmet": "6.0.0-beta",
    "react-html-parser": "^2.0.2",
    "react-intl": "2.8.0",
    "react-redux": "^7.0.2",
    "react-remarkable": "^1.1.3",
    "react-router-dom": "5.0.0",
    "react-vis": "^1.11.7",
    "redux": "4.0.1",
    "redux-saga": "1.0.2",
    "reselect": "4.0.0",
    "sanitize.css": "8.0.0",
    "styled-components": "4.2.0",
    "topojson-client": "^3.1.0",
    "url-search-params-polyfill": "^8.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.4.3",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.0",
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "3.0.1",
    "babel-plugin-styled-components": "1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "circular-dependency-plugin": "5.0.2",
    "compare-versions": "3.4.0",
    "compression-webpack-plugin": "2.0.0",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.6.0",
    "eslint-plugin-redux-saga": "1.0.0",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.6.0",
    "imports-loader": "0.8.0",
    "lint-staged": "13.1.0",
    "ngrok": "5.0.0-beta.2",
    "node-plop": "0.18.0",
    "null-loader": "0.1.1",
    "offline-plugin": "5.0.6",
    "plop": "2.3.0",
    "pre-commit": "1.2.2",
    "prettier": "1.17.0",
    "react-app-polyfill": "0.2.2",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "6.1.2",
    "rimraf": "2.6.3",
    "shelljs": "^0.8.5",
    "style-loader": "0.23.1",
    "stylelint": "10.0.1",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.6.0",
    "svg-url-loader": "2.3.2",
    "terser-webpack-plugin": "1.2.3",
    "url-loader": "1.1.2",
    "webpack": "4.30.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "3.6.2",
    "webpack-git-hash": "^1.0.2",
    "webpack-hot-middleware": "2.24.3",
    "webpack-pwa-manifest": "^4.2.0",
    "whatwg-fetch": "3.0.0"
  }
}
